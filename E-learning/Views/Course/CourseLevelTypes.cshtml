@model Dictionary<int, string>
@{
  ViewBag.Title = "CoursetypeTypes";
  Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
  <br />
  <h5>@Html.ActionLink(ViewBag.Kursi as String, "AllCourses", "Course", null, null)>@Html.ActionLink(ViewBag.Niveli as String, "CourseLevels", "Course", new { id = ViewBag.KursId }, null)</h5>
  <hr />
  <br />
  <h2 class="text-center">Tipet</h2>
  <br /><br />
  @if (Model == null || Model.Count() == 0)
  {
    <p>Ky nivel nuk ka asnje tip per momentin</p>
  }
  else
  {
    foreach (var type in Model)
    {
      @Html.ActionLink(type.Value, "MaterialsOfSection", "Material", new { @id = type.Key }, new { @class = "btn btn-dark btn-lg btn-block" })
    }
  }

</div>

